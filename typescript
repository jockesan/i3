icon="/home/jocke/images/lock/archer.jpg"
tmpbg=`mktemp /tmp/lockscreen.XXXXXXXXXX.png` || exit 1

scrot "$tmpbg"
convert "$tmpbg" "$icon" -blur 0x6  "$tmpbg"
i3lock -i "$tmpbg"
rm "$tmpbg"
