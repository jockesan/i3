#!/usr/bin/env bash

#icon="/home/jocke/images/lock/danger-zone.jpeg"
icon="/home/jocke/images/lock/archer.jpg"
tmpbg='/tmp/screen.png'

(( $# )) && { icon=$1; }

scrot "$tmpbg"
convert "$tmpbg" -scale 10% -scale 1000% "$tmpbg"
convert "$tmpbg" "$icon" -gravity center -composite -matte "$tmpbg"
i3lock -i "$tmpbg"
